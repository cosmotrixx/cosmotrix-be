{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/aurora-cron",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cme-cron",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/suv-cron",
      "schedule": "0 * * * *"
    }
  ]
}